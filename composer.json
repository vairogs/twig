{
    "name": "vairogs/twig",
    "description": "Useful Twig extensions",
    "type": "library",
    "keywords": [
        "env",
        "getenv",
        "php",
        "vairogs",
        "sort",
        "symfony",
        "twig",
        "twig-extension"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dāvis Zālītis (k0d3r1s)",
            "email": "k0d3r1s@vairogs.com",
            "homepage": "https://github.com/k0d3r1s",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@vairogs.com",
        "issues": "https://github.com/vairogs/vairogs/issues",
        "wiki": "https://github.com/vairogs/vairogs/wiki"
    },
    "funding": [
        {
            "type": "ko-fi",
            "url": "https://ko-fi.com/k0d3r1s"
        },
        {
            "type": "paypal",
            "url": "https://www.paypal.com/paypalme/k0d3r1s"
        },
        {
            "type": "issuehunt",
            "url": "https://issuehunt.io/r/vairogs/vairogs"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "doctrine/collections": "~1.6",
        "jetbrains/phpstorm-attributes": "*",
        "twig/twig": "3.*",
        "vairogs/utils": "3.x-dev"
    },
    "replace": {
        "vairogs/twig-getenv": "self.version",
        "vairogs/twig-helper": "self.version",
        "vairogs/twig-helper-date": "self.version",
        "vairogs/twig-helper-email": "self.version",
        "vairogs/twig-helper-file": "self.version",
        "vairogs/twig-helper-generator": "self.version",
        "vairogs/twig-helper-gravatar": "self.version",
        "vairogs/twig-helper-http": "self.version",
        "vairogs/twig-helper-iter": "self.version",
        "vairogs/twig-helper-json": "self.version",
        "vairogs/twig-helper-php": "self.version",
        "vairogs/twig-helper-sort": "self.version",
        "vairogs/twig-helper-text": "self.version",
        "vairogs/twig-helper-uri": "self.version",
        "vairogs/twig-helper-util": "self.version"
        "vairogs/twig-pagination": "self.version",
        "vairogs/twig-php": "self.version",
        "vairogs/twig-sort": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Vairogs\\Twig\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        },
        "thanks": {
            "name": "vairogs/vairogs",
            "url": "https://github.com/vairogs/vairogs"
        }
    }
}
